{
  "extends": "recommended",
  "rules": {
    "CompileStatic": {
      "enabled": false
    },
    "DuplicateStringLiteral": "off",
    "LineLength": "off",
    "MethodReturnTypeRequired": "off",
    "NestedBlockDepth": "off",
    "NoDef": "off",
    "formatting.Indentation": {
      "spacesPerIndentLevel": 4,
      "severity": "info"
    }
  }
}
